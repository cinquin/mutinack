#!/bin/bash
(nohup java `echo -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=``python -c 'import socket; s=socket.socket(); s.bind(("", 0)); print(s.getsockname()[1]); s.close()'` -Djavax.net.ssl.trustStore=mutinack_public_trustStore -Djavax.net.ssl.trustStorePassword=changeit -Djavax.net.ssl.keyStore=mutinack_public_selfsigned.jks -Djavax.net.ssl.keyStorePassword=passphrase -Djava.security.policy==java.policy -XX:+UseParallelGC -XX:ParallelGCThreads=3 -XX:CICompilerCount=3 -javaagent:lib_build_run_test/org.jacoco.agent-0.7.5.201505241946-runtime.jar=destfile=target/jacoco.exec -jar mutinack.jar -startWorker localhost/mutinackTesting:2 &> run_worker_out) &
echo $!
